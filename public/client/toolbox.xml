<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">

  <sep></sep>
  <category name="Math" colour="#5C68A6">
    <block type="math_round">
      <field name="OP">ROUND</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">3.1</field>
        </shadow>
      </value>
    </block>
    <block type="math_number">
      <field name="NUM">0</field>
    </block>
    <block type="math_single">
      <field name="OP">ROOT</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">9</field>
        </shadow>
      </value>
    </block>
    <block type="math_trig">
      <field name="OP">SIN</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">45</field>
        </shadow>
      </value>
    </block>
    <block type="math_constant">
      <field name="CONSTANT">PI</field>
    </block>
    <block type="math_number_property">
      <mutation divisor_input="false"></mutation>
      <field name="PROPERTY">EVEN</field>
      <value name="NUMBER_TO_CHECK">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="math_arithmetic">
      <field name="OP">ADD</field>
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="math_on_list">
      <mutation op="SUM"></mutation>
      <field name="OP">SUM</field>
    </block>
    <block type="math_modulo">
      <value name="DIVIDEND">
        <shadow type="math_number">
          <field name="NUM">64</field>
        </shadow>
      </value>
      <value name="DIVISOR">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="math_constrain">
      <value name="VALUE">
        <shadow type="math_number">
          <field name="NUM">50</field>
        </shadow>
      </value>
      <value name="LOW">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="HIGH">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="math_random_int">
      <value name="FROM">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="TO">
        <shadow type="math_number">
          <field name="NUM">100</field>
        </shadow>
      </value>
    </block>
    <block type="math_random_float"></block>
  </category>
  
  <category name="Variables" colour="#A65C81" custom="VARIABLE"></category>

  <category name="Vectors" colour="#5CA68F">
    <block type="vector2Field">
      <field name="x">0</field>
      <field name="y">0</field>
    </block>
    <block type="vector3Field">
      <field name="x">0</field>
      <field name="y">0</field>
      <field name="z">0</field>
    </block>
    <block type="vector2">
      <field name="x">Infinity</field>
      <field name="y">Infinity</field>
    </block>
    <block type="vector3">
      <field name="x">Infinity</field>
      <field name="y">Infinity</field>
      <field name="z">Infinity</field>
    </block>
    <block type="points"></block>
  </category>

  <sep></sep>

  <category name="Loops" colour="#5CA65C">
    <block type="forLoop"></block>
  </category>

  <sep></sep>

  <category name="Features" colour="#5ba55b">
    <block type="feature">
      <field name="featurename">Blockly Created Feature</field>
      <field name="preconditions"></field>
      <field name="actions"></field>
    </block>
    <block type="preconditions"></block>
    <block type="unionall"></block>
    <block type="colorPart"></block>
  </category>


  <category name="Sketch" colour="FF0004">
    <block type="sketch">
      <field name="skname">Sketch1</field>
      <field name="planename">Top</field>
      <field name="skentities"></field>
    </block>
    <block type='plane'></block>
    <sep></sep>
    <sep></sep>
    <block type="skcircle">
      <field name="center_xy">Infinity</field>
      <field name="radius">Infinity</field>
    </block>
    <block type="skrectangle">
      <field name="first_corner_xy">Infinity</field>
      <field name="second_corner_xy">Infinity</field>
    </block>
    <block type="skregularpolygon">
      <field name="center_xy">Infinity</field>
      <field name="vertex_xy">Infinity</field>
      <field name="sides">Infinity</field>
    </block>
    <block type="skpoint">
      <field name="position_xy">Infinity</field>   
    </block>
    <block type="sklinesegment">
      <field name="start_xy">Infinity</field>
      <field name="end_xy">Infinity</field>
    </block>
    <block type="skpolyline"></block>
    <block type="sktext">
      <field name="text">Example</field>
    </block>
    <block type="skellipse">
      <field name="center_xy">Infinity</field>
      <field name="majorRadius">Infinity</field>
      <field name="minorRadius">Infinity</field>
    </block>
    <block type="skarc">
      <field name="start_xy">Infinity</field>
      <field name="mid_xy">Infinity</field>
      <field name="end_xy">Infinity</field>
    </block>
    <block type="skellipticalarc">
      <field name="center_xy">Infinity</field>
      <field name="major_xy">Infinity</field>
      <field name="majorRadius">Infinity</field>
      <field name="minorRadius">Infinity</field>
      <field name="startParameter">Infinity</field>
      <field name="endParameter">Infinity</field>
    </block>
  </category>


  <category name="3D Shapes" colour="002AFF">
    <block type="fcuboid">
      <field name="corner1_xyz">Infinity</field>
      <field name="corner2_xyz">Infinity</field>
    </block>
    <block type="fcylinder">
      <field name="top_xyz">Infinity</field>
      <field name="bottom_xyz">Infinity</field>
      <field name="radius">Infinity</field>
    </block>
    <block type="opsphere">
      <field name="center_xyz">Infinity</field>
      <field name="diameter">Infinity</field>
    </block>
    <block type="fcone">
      <field name="top_xyz">Infinity</field>
      <field name="bottom_xyz">Infinity</field>
      <field name="topRadius">Infinity</field>
      <field name="bottomRadius">Infinity</field>
    </block>
    <block type="fellipsoid">
      <field name="center_xyz">Infinity</field>
      <field name="radial_xyz">Infinity</field>
    </block>
  </category>

  <category name="GeomOperations" colour="#745CA6">
    <block type='extrude'>
      <field name="FIELDNAME">LANGUAGE-NEUTRAL-KEY</field>
    </block>
    <block type='revolve'></block>
    <block type='sweep'></block>
    <block type='loft'></block>
  </category>


</xml>
